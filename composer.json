{
    "name": "havokinspiration/wrench",
    "description": "Maintenance mode plugin for CakePHP 3.X",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yves Piquel",
            "homepage": "https://github.com/HavokInspiration/wrench"
        }
    ],
    "support": {
        "issues": "https://github.com/HavokInspiration/wrench/issues",
        "source": "https://github.com/HavokInspiration/wrench"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "dev-3.next as 3.3.0-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/bake": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Wrench\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wrench\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "App\\": "tests/test_app/App"
        }
    }
}
